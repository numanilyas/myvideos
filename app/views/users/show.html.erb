<% provide(:title, @user.name) %>
<div class="row">
  <aside class="span3">
    <section>
      <h4>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h4>
      <div class="row">
      	<div class="span3">
      		<h5>Albums</h5>
      	</div>
      	<%= render(@user.video_categories)%>
      </div>
      <h6><%= link_to "New Album", newalbum_path %></h6>
    </section>
  </aside>
  <div class="span9">
  	<% if @album_videos.nil?%>
  		<%= render(@videos)%>
  	<% else %>
  		<%= render(@album_videos)%>
  	<% end %>
  	<h6><%= link_to "New Video", newvideo_path%></h6>
  </div>
</div>